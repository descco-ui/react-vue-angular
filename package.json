{
  "name": "@tgkr/components",
  "version": "0.1.0",
  "description": "UI Components",
  "main": "lib/index.js",
  "scripts": {
    "storybook:vanilla": "node ./node_modules/@storybook/html/dist/server --static-dir ./node_modules,./source/vanilla --port 9000 --config-dir ./internals/storybook/vanilla",
    "storybook:react": "node ./node_modules/@storybook/react/dist/server -p 9001 -c ./internals/storybook/react/",
    "storybook:vue": "node ./node_modules/@storybook/vue/dist/server -p 9002 -c ./internals/storybook/vue/",
    "storybook:angular": "node ./node_modules/@storybook/angular/dist/server -p 9003 -c ./internals/storybook/angular/",
    "build-storybook:vanilla": "cross-env NODE_ENV=production node ./node_modules/@storybook/html/dist/server/build --config-dir ./internals/storybook/vanilla --output-dir ./lib/storybook/vanilla",
    "build-storybook:react": "cross-env NODE_ENV=production node ./node_modules/@storybook/react/dist/server/build -c ./internals/storybook/react/ -o ./lib/storybook/react",
    "build-storybook:vue": "cross-env NODE_ENV=production node ./node_modules/@storybook/vue/dist/server/build -c ./internals/storybook/vue/ -o ./lib/storybook/vue",
    "build-storybook:angular": "cross-env NODE_ENV=production node ./node_modules/@storybook/angular/dist/server/build -c ./internals/storybook/angular/ -o ./lib/storybook/angular",
    "release": "./node_modules/.bin/release-it --no-npm.publish --git.tagName='v${version}' --github.release"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgkr/components.git"
  },
  "keywords": [
    "togakure",
    "ui",
    "vanilla",
    "react",
    "vue",
    "angular"
  ],
  "author": "Hemerson Vianna <hemerson.lourenco@gmail.com> (https://nerdcalistenico.com.br)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgkr/components/issues"
  },
  "homepage": "https://github.com/tgkr/components#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.23",
    "@angular/cli": "8.3.23",
    "@angular/compiler-cli": "8.2.14",
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@dump247/storybook-state": "1.6.1",
    "@storybook/addon-actions": "5.3.9",
    "@storybook/addon-backgrounds": "5.3.9",
    "@storybook/addon-console": "1.2.1",
    "@storybook/addon-events": "5.3.9",
    "@storybook/addon-info": "5.3.9",
    "@storybook/addon-knobs": "5.3.9",
    "@storybook/addon-links": "5.3.9",
    "@storybook/addon-notes": "5.3.9",
    "@storybook/addon-options": "5.3.9",
    "@storybook/addon-viewport": "5.3.9",
    "@storybook/addons": "5.3.9",
    "@storybook/angular": "5.3.9",
    "@storybook/html": "5.3.9",
    "@storybook/react": "5.3.9",
    "@storybook/vue": "5.3.9",
    "@webcomponents/webcomponentsjs": "2.4.1",
    "babel-loader": "8.0.6",
    "babel-plugin-react-css-modules": "5.2.6",
    "babel-preset-vue": "2.0.2",
    "css-loader": "3.4.2",
